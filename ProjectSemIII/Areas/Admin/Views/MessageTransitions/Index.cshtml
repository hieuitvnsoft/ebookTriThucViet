@model IEnumerable<ProjectSemIII.Areas.Admin.Models.DataModel.MessageTransition>


@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Customer.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model=>model.BankId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccountHoder)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeTrans)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Customer.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BankId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccountHoder)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeTrans)
        </td>
        
        <td>
          
            @Html.ActionLink("Đọc", "Details", new { id=item.Id },new {@class="btn btn-primary" }) |
           
        </td>
    </tr>
}

</table>
